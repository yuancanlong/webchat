page{
  background: #eee;
}
.row{
  overflow: hidden;
  line-height:50px;
  border-bottom:1px solid #ccc;
  color:#777;
  background: #fff;
}
.submit-button{
  margin:10px 8px 0 8px;
  background:#ff9700;
  height:38px;
  line-height: 38px;
  border-radius:6px;
  text-align: center;
  color:#fff;
}
.title{
  float: left;
  width:90px;
  padding-left:10px;
}
.info-input{
  height:50px;
}
.success{
  background: #fff;
  padding-bottom:20px;
}
.gongxi{
  text-align: center;
  line-height: 100px;
  font-size:16px;
}
.success-icon{
  position:relative;
  top:6px;
  margin-right:10px;
}
.back-button{
  margin:0 10px;
}